<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd        ">

    <dubbo:application name="hello-world-app" />

    <!--<dubbo:registry protocol="multicast" address="multicast://224.5.6.7:1234"/>-->
    <dubbo:registry protocol="zookeeper" address="localhost:2181"/>
    <dubbo:protocol name="dubbo" port="20880" />

    <!-- 暴露服务 -->
    <bean id="demoServiceImpl" class="wjc.dubbo.demo.service.DemoServiceImpl" />
    <dubbo:service interface="wjc.dubbo.demo.api.DemoService" ref="demoServiceImpl"/>
</beans>